use Base.dg
use Math.dg

namespace Floats{

	public ()~>PInt main{
		println("PI/2");
		printfl(subr(3.14159,2.0));

		println("3.5*2");
		printfl(3.5*2.0);

		println("6.0+2.0");
		printfl(6.0+2.0);

		println("2.0-4.0");
		printfl(2.0-4.0);
