public class Array1{
	public PInt main(){

		arr=[1,2,3,4];

		//x=arr[0];
		printarr(arr,4);
		arr2=reversearr(arr,4);
		printarr(arr2,4);
		return 0;	

	}
	public [PInt] reversearr([PInt] arr,PInt size){
		//modified
		arr2=[0,0,0,0];
		i=0;
		while(i<size){

			arr2[i]=arr[size-i-1];
			i=i+1;
		}
		return arr2;
	}
	public PInt printarr([PInt] arr,PInt size){
		
		i=0;
		while(i<size){

			putdigit(arr[i]);
			putchar(',');
			i=i+1;
		}
		putchar(' ');
		return 0;
	}
}
