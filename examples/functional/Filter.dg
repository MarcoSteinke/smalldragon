
	fn main ()~>PInt {
		arr=[1,2];

		arr2 = filter(arr,myfilter);
		

		putdigit(arr2[0]);

		putdigit(arr2[1]);

		return 0;
	}

	fn myfilter (PInt item)->Bool {
		return (item==2);	
	}

	fn filter ([PInt] arr, ( (PInt)->Bool ) myfilterr)->[PInt] {
		arr2 = [0,0];

		i=0;

		while(i<2){

			if(myfilterr(arr[i])){
				arr2[i]=arr[i];
			}
			i=i+1;
		}

		return arr2;
	}
