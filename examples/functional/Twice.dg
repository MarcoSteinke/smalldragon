
struct Simple {
	((PInt) -> PInt)  member;
}

fn main ()~>PInt {

	printf("%d\n",subr(id));
	printf("%d\n",subr(twice));

	((PInt) -> PInt) myfunction = id;

	myfunction(3);

	Simple s = newSimple();

	s.member = myfunction;

	delSimple(s);

	return 0;
}

fn id (PInt n)->PInt {
	return n;
}

fn twice (PInt n)->PInt {return 2*n;}

fn subr (((PInt)->PInt) subr1)->PInt {
	return subr1(2);
}

