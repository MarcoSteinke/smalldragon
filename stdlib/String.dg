
fn substr ([char] s, uint i, uint j)->[char] {
	uint l=abs(j-i);
	[char] res=new(l);
	uint k=0;
	while(k<l){
		res[k]=s[i+k];
		k=k+1;
	}
	return res;
}

fn trimleft ([char] s)->[char] {
	
	i=0;
	blank=true;
	while( (i<len(s)) && blank){
		if(s[i] != ' '){
			blank=false;
		}else{
			i=i+1;
		}
	}

	[char] r = new(len(s)-i);
	k=0;
	while(i<len(s)){
		r[k]=s[i];
		k=k+1;
		i=i+1;
	}
	return r;
}

