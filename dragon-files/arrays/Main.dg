public class Main{
	public Void main(){

		arr=[1,2,3,4];

		//x=arr[0];
		printarr(arr,4);
		arr2=reversearr(arr,4);
		printarr(arr2,4);
		

	}
	public [Int] reversearr([Int] arr,Int size){
		//modified
		arr2=[0,0,0,0];
		i=0;
		while(i<size){

			arr2[i]=arr[size-i-1];
			i=i+1;
		}
		return arr2;
	}
	public Void printarr([Int] arr,Int size){
		
		i=0;
		while(i<size){

			print(arr[i]);
			print(",");
			i=i+1;
		}
		println();
	}
}
