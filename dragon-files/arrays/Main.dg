public class Main{
	public Void main(){

		arr=[1];

		//x=arr[0];
		printarr(arr,1);
		arr2=reversearr(arr,1);
		printarr(arr2,1);
		

	}
	public [Int] reversearr([Int] arr,Int size){
		//modified
		arr2=[0];
		i=0;
		while(i<size){

			arr2[i]=i;
			i=i+1;
		}
		return arr2;
	}
	public Void printarr([Int] arr,Int size){
		
		i=0;
		while(i<size){

			println(arr[i]);
			i=i+1;
		}
	}
}
